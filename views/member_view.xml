<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="member_class_action" model="ir.actions.act_window">
        <field name="name">Daftar Member</field>
        <field name="res_model">member.class</field>
        <field name="view_mode">tree,form</field>
    </record>

    <record id="member_class_view_tree" model="ir.ui.view">
        <field name="name">member.class.view.tree</field>
        <field name="model">member.class</field>
        <field name="arch" type="xml">
            <tree string="">
                 <field name="nis"/>
                 <field name="name"/>
                 <field name="grade"/>
            </tree>
        </field>
    </record>

    <record id="member_class_view_form" model="ir.ui.view">
        <field name="name">member.class.view.form</field>
        <field name="model">member.class</field>
        <field name="arch" type="xml">
            <form string="">
                <sheet>
                    <group>
                        <field name="nis"/>
                        <field name="name"/>
                        <field name="grade"/>
                    </group>
                </sheet>
            </form>
        </field>
    </record>

</odoo>
